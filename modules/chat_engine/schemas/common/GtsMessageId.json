  "$id": "gts://gts.hyperspot.chat_engine.entities.message_id.v1~",
  "title": "GTS Message ID",
  "description": "GTS ID for a message instance",
  "type": "string",
  "pattern": "^gts\\.hyperspot\\.chat_engine\\.entities\\.message\\.v[0-9]+~[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
