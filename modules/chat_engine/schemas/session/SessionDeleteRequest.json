{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "gts://gts.x.chat_engine.session.delete_request.v1~",
  "title": "SessionDeleteRequest",
  "description": "Request to delete a session",
  "type": "object",
  "properties": {
    "session_id": {
      "type": "string",
      "format": "uuid",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "description": "Session identifier to delete"
    }
  },
  "required": ["session_id"],
  "additionalProperties": false
}
