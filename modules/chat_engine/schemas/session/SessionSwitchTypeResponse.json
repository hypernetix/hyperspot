{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "gts://gts.x.chat_engine.session.switch_type_response.v1~",
  "title": "SessionSwitchTypeResponse",
  "description": "Response after switching session type",
  "type": "object",
  "properties": {
    "session_id": {
      "type": "string",
      "format": "uuid",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "description": "Session identifier"
    },
    "session_type_id": {
      "type": "string",
      "format": "uuid",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "description": "New session type ID"
    }
  },
  "required": ["session_id", "session_type_id"],
  "additionalProperties": false
}
