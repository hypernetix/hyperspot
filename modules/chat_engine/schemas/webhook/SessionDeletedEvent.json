{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "gts://gts.x.chat_engine.webhook.session_deleted_event.v1~",
  "title": "SessionDeletedEvent",
  "description": "Webhook event sent when a session is deleted",
  "type": "object",
  "properties": {
    "event": {
      "type": "string",
      "const": "session.deleted",
      "description": "Event type identifier"
    },
    "session_id": {
      "type": "string",
      "format": "uuid",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "description": "Deleted session identifier"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "Event timestamp",
      "examples": ["2024-01-15T10:30:00Z"]
    }
  },
  "required": ["event", "session_id", "timestamp"],
  "additionalProperties": false
}
